<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yh.cn.dao.CarCountDao">

    <update id="subtraction" parameterType="java.lang.Integer">
        update car_count
        set count=count-#{countNum}
        where id = '1'
    </update>

</mapper>